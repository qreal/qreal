					</picture>
					<labels>
						<label x="5" y="3" textBinded="name" center="true"/>
					</labels>
					<ports>
						<linePort>
							<start startx="0" starty="10"/>
							<end endx="0" endy="90"/>
						</linePort>
						<linePort>
							<start startx="150" starty="10"/>
							<end endx="150" endy="90"/>
						</linePort>
						<linePort>
							<start startx="10" starty="0"/>
							<end endx="140" endy="0"/>
						</linePort>
						<linePort>
							<start startx="10" starty="100"/>
							<end endx="140" endy="100"/>
						</linePort>
					</ports>
				</graphics>
				<logic>
					<generalizations>
