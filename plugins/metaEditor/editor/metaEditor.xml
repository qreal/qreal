						<contains type="MetaEditor::Inheritance"/>
						<contains type="MetaEditor::MetaEntityPossibleEdge"/>
						<contains type="MetaEditor::MetaEntityPropertiesAsContainer"/>
						<properties>
							<sortContainer/>
							<minimizeToChildren/>
							<banChildrenMove/>
							<forestalling size="25,35,25,25"/>
