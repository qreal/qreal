					</labels>
					<ports>
						<linePort>
							<start startx="0" starty="35"/>
							<end endx="0" endy="115"/>
						</linePort>
						<linePort>
							<start startx="150" starty="35"/>
							<end endx="150" endy="115"/>
						</linePort>
					</ports>
				</graphics>
				<logic>
					<generalizations>
						<parent parentName="KernelDiagram::Diagram" overrides="all"/>
					</generalizations>
					<properties/>
