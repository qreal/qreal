<?xml version="1.0" encoding="utf-8"?>
<metamodel xmlns="http://schema.real.com/schema/">
	<namespace>UML 2.0</namespace>
	<diagram nodeName="DatabasesNode" name="Databases" displayedName="Logical model">
		<nonGraphicTypes>
			<port name="EntityLeft"/>
			<port name="EntityVertical"/>
			<port name="EntityRight"/>
			<enum name="DataTypes" editable="true">
				<value name="BOOL" displayedName="BOOL"/>
				<value name="CHAR" displayedName="CHAR()"/>
				<value name="DATETIME" displayedName="DATETIME"/>
				<value name="DECIMAL" displayedName="DECIMAL(,)"/>
				<value name="FLOAT" displayedName="FLOAT(,)"/>
				<value name="INT" displayedName="INT"/>
				<value name="NTEXT" displayedName="NTEXT"/>
				<value name="NVARCHAR" displayedName="NVARCHAR()"/>
				<value name="TEXT" displayedName="TEXT"/>
			</enum>
		</nonGraphicTypes>
		<graphicTypes>
			<node name="DatabasesNode" displayedName="Logical model">
				<graphics>
					<picture/>
					<labels/>
					<ports/>
				 </graphics>
				<logic>
					<container>
						<contains type="Databases::Entity"/>
						<contains type="Databases::Attribute"/>
						<contains type="Databases::OneToOneRelationship"/>
						<contains type="Databases::OneToManyRelationship"/>
						<contains type="Databases::ManyToManyRelationship"/>
					</container>
				</logic>
			</node>
			<node name="Entity" displayedName="Entity">
				<graphics>
					<picture sizex="160" sizey="90">
						<rectangle stroke-style="solid" stroke="#000000" y1="0" stroke-width="0" x1="0" y2="90" x2="160"/>
					</picture>
					<labels>
						<label x="5a" y="3a" textBinded="entityName" isFocused="true"/>
					</labels>
					<ports>
						<linePort type="EntityLeft">
							<start startx="0" starty="0"/>
							<end endx="0" endy="90"/>
						</linePort>
						<linePort type="EntityRight">
							<start startx="160" starty="0"/>
							<end endx="160" endy="90"/>
						</linePort>
						<linePort type="EntityVertical">
							<start startx="0" starty="0"/>
							<end endx="160" endy="0"/>
						</linePort>
						<linePort type="EntityVertical">
							<start startx="0" starty="90"/>
							<end endx="160" endy="90"/>
						</linePort>
					</ports>
				</graphics>
				<logic>
					<container>
						<contains type="Databases::Attribute"/>
						 <properties>
							<sortContainer/>
							<minimizeToChildren/>
							<banChildrenMove/>
							<forestalling size="0,35,0,0"/>
							<childrenForestalling size="0"/>
						 </properties>
					</container>
					<properties>
						<property type="string" name="entityName" displayedName="Name">
							<default></default>
						</property>
					</properties>
				</logic>
			</node>
			<node name="Attribute" displayedName="Attribute">
				<graphics>
					<picture sizex="150" sizey="31">
						<rectangle stroke-style="solid" stroke="#000000" y1="0" stroke-width="0" x1="0" y2="31" x2="150"/>
						<rectangle stroke-style="solid" stroke="#000000" y1="8" stroke-width="0" x1="136" y2="13" x2="140">
							<showIf value="true" property="isPrimaryKey" sign="="/>
						</rectangle>
						<ellipse fill="#ffffff" stroke="#000000" stroke-style="solid" stroke-width="0" y1="4" y2="17" fill-style="none" x1="132" x2="144">
							<showIf value="true" property="isPrimaryKey" sign="="/>
						</ellipse>
						<line fill="#ffffff" stroke="#000000" stroke-style="solid" stroke-width="0" y1="17" y2="25" fill-style="none" x1="139" x2="139">
							<showIf value="true" property="isPrimaryKey" sign="="/>
						</line>
						<line fill="#ffffff" stroke="#000000" stroke-style="solid" stroke-width="0" y1="17" y2="17" fill-style="none" x1="137" x2="139">
							<showIf value="true" property="isPrimaryKey" sign="="/>
						</line>
						<line fill="#ffffff" stroke="#000000" stroke-style="solid" stroke-width="0" y1="17" y2="25" fill-style="none" x1="137" x2="137">
							<showIf value="true" property="isPrimaryKey" sign="="/>
						</line>
						<line fill="#ffffff" stroke="#000000" stroke-style="solid" stroke-width="0" y1="23" y2="23" fill-style="none" x1="134" x2="136">
							<showIf value="true" property="isPrimaryKey" sign="="/>
						</line>
						<line fill="#ffffff" stroke="#000000" stroke-style="solid" stroke-width="0" y1="20" y2="20" fill-style="none" x1="133" x2="136">
							<showIf value="true" property="isPrimaryKey" sign="="/>
						</line>
					</picture>
					<labels>
						<label x="10a" y="1a" textBinded="attributeName" isFocused="true"/>
					</labels>
					<nonResizeable/>
				</graphics>
				<logic>
					<properties>
						<property type="string" name="attributeName" displayedName="Name">
							<default></default>
						</property>
						<property type="bool" name="isPrimaryKey" displayedName="Primary Key"/>
						<property type="bool" name="isUnique" displayedName="Unique"/>
						<property type="bool" name="notNull" displayedName="Not null"/>
						<property type="string" name="DataType" type="DataTypes" displayedName="Data Type">
							<default>INT</default>
						</property>
					</properties>
				</logic>
			</node>
			<edge name="ManyToManyRelationship" displayedName="Many-to-many">
				<logic>
					<dividability isDividable="true"/>
					<associations endType="many_relationship" beginType="many_relationship">
						<association endName="end" beginName="start"/>
					</associations>
					<properties>
						<property type="string" displayedName="Name" name="Name"/>
						<property type="string" displayedName="Table name" name="tableName"/>
					</properties>
					<fromPorts>
						<port type="EntityRight"/>
						<port type="EntityVertical"/>
						<port type="EntityLeft"/>
					</fromPorts>
					<toPorts>
						<port type="EntityRight"/>
						<port type="EntityVertical"/>
						<port type="EntityLeft"/>
					</toPorts>
					<possibleEdges>
						<possibleEdge beginName="EntityRight" directed="true" endName="EntityLeft"/>
						<possibleEdge beginName="EntityRight" directed="true" endName="EntityRight"/>
						<possibleEdge beginName="EntityRight" directed="true" endName="EntityVertical"/>
						<possibleEdge beginName="EntityLeft" directed="true" endName="EntityLeft"/>
						<possibleEdge beginName="EntityLeft" directed="true" endName="EntityVertical"/>
						<possibleEdge beginName="EntityVertical" directed="true" endName="EntityVertical"/>
					</possibleEdges>
				</logic>
				<graphics>
					<lineType type="solidLine"/>
					<labels>
						<label textBinded="Name"/>
					</labels>
				</graphics>
			</edge>
			<edge name="OneToManyRelationship" displayedName="One-to-many">
				<graphics>
					<lineType type="solidLine"/>
					<labels>
						<label textBinded="Name"/>
					</labels>
				</graphics>
				<logic>
					<dividability isDividable="true"/>
					<associations endType="one_relationship" beginType="many_relationship">
						<association endName="end" beginName="start"/>
					</associations>
					<properties>
						<property type="string" displayedName="Name" name="Name"/>
						<property type="string" displayedName="Column name" name="columnName"/>
					</properties>
					<fromPorts>
						<port type="EntityRight"/>
						<port type="EntityVertical"/>
						<port type="EntityLeft"/>
					</fromPorts>
					<toPorts>
						<port type="EntityRight"/>
						<port type="EntityVertical"/>
						<port type="EntityLeft"/>
					</toPorts>
					<possibleEdges>
						<possibleEdge beginName="EntityRight" directed="true" endName="EntityLeft"/>
						<possibleEdge beginName="EntityRight" directed="true" endName="EntityRight"/>
						<possibleEdge beginName="EntityRight" directed="true" endName="EntityVertical"/>
						<possibleEdge beginName="EntityLeft" directed="true" endName="EntityLeft"/>
						<possibleEdge beginName="EntityLeft" directed="true" endName="EntityVertical"/>
						<possibleEdge beginName="EntityVertical" directed="true" endName="EntityVertical"/>
					</possibleEdges>
				</logic>
			</edge>
			<edge name="OneToOneRelationship" displayedName="One-to-one">
				<graphics>
					<lineType type="solidLine"/>
					<labels>
						<label textBinded="Name"/>
					</labels>
				</graphics>
				<logic>
					<dividability isDividable="true"/>
					<associations endType="one_relationship" beginType="one_relationship">
						<association endName="end" beginName="start"/>
					</associations>
					<properties>
						<property type="string" displayedName="Name" name="Name"/>
					</properties>
					<fromPorts>
						<port type="EntityRight"/>
						<port type="EntityVertical"/>
						<port type="EntityLeft"/>
					</fromPorts>
					<toPorts>
						<port type="EntityRight"/>
						 <port type="EntityVertical"/>
						<port type="EntityLeft"/>
					</toPorts>
					<possibleEdges>
						<possibleEdge beginName="EntityRight" directed="true" endName="EntityLeft"/>
						<possibleEdge beginName="EntityRight" directed="true" endName="EntityRight"/>
						<possibleEdge beginName="EntityRight" directed="true" endName="EntityVertical"/>
						<possibleEdge beginName="EntityLeft" directed="true" endName="EntityLeft"/>
						<possibleEdge beginName="EntityLeft" directed="true" endName="EntityVertical"/>
						<possibleEdge beginName="EntityVertical" directed="true" endName="EntityVertical"/>
					</possibleEdges>
				</logic>
			</edge>
		</graphicTypes>
		<palette>
		<group name="Entities and attributes">
				<element name="Entity"/>
				<element name="Attribute"/>
			</group>
			<group name="Relationships">
				<element name="OneToOneRelationship"/>
				<element name="OneToManyRelationship"/>
				<element name="ManyToManyRelationship"/>
			</group>
		</palette>
	</diagram>
</metamodel>
